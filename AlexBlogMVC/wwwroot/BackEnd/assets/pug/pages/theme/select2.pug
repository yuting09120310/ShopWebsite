- var select2               = true;
- var page_select2          = true;
- var theme_customizer      = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-6
                  h3
                    | Select2

                .col-6
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Form Widgets
                    li.breadcrumb-item.active Select2
          // Container-fluid starts
          .container-fluid
            .select2-drpdwn
              .row
                // Default Textbox start
                .col-md-12
                  .card
                    .card-header
                      h5.card-title Select-2
                    .card-body.o-hidden
                      .mb-2
                        .col-form-label Default Placeholder
                        select.js-example-placeholder-multiple.col-sm-12(multiple='multiple')
                          option(value='AL') Alabama
                          option(value='WY') Wyoming
                          option(value='WY') Coming
                          option(value='WY') Hanry Die
                          option(value='WY') John Doe
                      .mb-2
                        .col-form-label Select2 single select
                        select.js-example-basic-single.col-sm-12
                          optgroup(label='Developer')
                            option(value='AL') Alabama
                            option(value='WY') Wyoming
                          optgroup(label='Designer')
                            option(value='WY') Peter
                            option(value='WY') Hanry Die
                            option(value='WY') John Doe
                      .mb-2
                        .col-form-label Select2 multi select
                        select.js-example-basic-multiple.col-sm-12(multiple='multiple')
                          option(value='AL') Alabama
                          option(value='WY') Wyoming
                          option(value='WY') Coming
                          option(value='WY') Hanry Die
                          option(value='WY') John Doe
                      .mb-2
                        .col-form-label RTL support
                        select.js-example-rtl.col-sm-12(multiple='multiple')
                          option(value='AL') Alabama
                          option(value='WY') Wyoming
                          option(value='WY') Coming
                          option(value='WY') Hanry Die
                          option(value='WY') John Doe
                      .mb-2
                        .col-form-label Limiting The Number Of Selections
                        select.js-example-basic-multiple-limit.col-sm-12(multiple='multiple')
                          option(value='AL') Alabama
                          option(value='WY') Wyoming
                          option(value='WY') Coming
                          option(value='WY') Hanry Die
                          option(value='WY') John Doe
                      .mb-2
                        .col-form-label Disabled Results
                        select.js-example-disabled-results.col-sm-12
                          option(value='one') First
                          option(value='two' disabled='disabled')
                            | Second
                            | (disabled)
                          option(value='three') Third
                      .mb-2
                        .col-form-label Hiding The Search Box
                        select.js-example-basic-hide-search.col-sm-12(multiple='multiple')
                          optgroup(label='Developer')
                            option(value='AL') Smith
                            option(value='WY') Peter
                            option(value='WY') James
                            option(value='WY') Hanry Die
                            option(value='WY') John Doe
                            option(value='WY') Harry Poter
                      div
                        .col-form-label Enable-Disable
                        select.js-example-disabled.col-sm-10
                          option(value='AL') Smith
                          option(value='WY') Peter
                          option(value='WY') James
                          option(value='WY') Hanry Die
                          option(value='WY') John Doe
                          option(value='WY') Harry Poter
                        div.text-end.mt-2
                          button.btn.btn-primary.js-programmatic-enable.p-2.mt-2
                            | Enable
                          button.btn.btn-danger.js-programmatic-disable.p-2.mt-2
                            | Disable
